# Supabase Configuration
# Get these values from your Supabase project settings:
# https://app.supabase.com/project/_/settings/api

# Supabase Project URL
VITE_SUPABASE_URL=https://your-project-id.supabase.co

# Supabase Anonymous/Public Key (safe to use in frontend)
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Optional: Enable offline-first mode (keeps localStorage as cache)
VITE_ENABLE_OFFLINE_MODE=true

# Logging Configuration
# Enable debug logs (automatically enabled in development and staging)
VITE_ENABLE_DEBUG_LOGS=false

# Vercel environment (set automatically by Vercel)
# Values: production | preview | development
VITE_VERCEL_ENV=${VERCEL_ENV}
